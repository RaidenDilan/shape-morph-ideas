{
  "name": "shape-morph-ideas",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hookform/resolvers": "2.8.8",
    "@types/react": "18.0.9",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "animejs": "3.2.1",
    "node-sass": "7.0.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-router-dom": "6.3.0",
    "react-transition-group": "4.4.2",
    "sass": "1.51.0",
    "typescript": "4.6.4",
    "vite": "2.9.9",
    "vite-plugin-svgr": "2.1.0",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "env-cmd -f ./environments/.dev.env vite",
    "prod": "env-cmd -f ./environments/.prod.env vite",
    "staging": "env-cmd -f ./environments/.staging.env vite",
    "integration": "env-cmd -f ./environments/.integration.env vite",
    "start": "vite",
    "preview": "vite preview",
    "build": "tsc && vite build",
    "lighthouse": "env-cmd -f ./environments/.prod.env vite build && vite preview",
    "unit": "tsc && jest --coverage --updateSnapshot",
    "format:fix": "prettier --write \"{src,__tests__,.storybook}/**/*.{ts,tsx,scss,js}\" \"**/*.yml\" \"**/*.json\" \"README.md\" ",
    "format:check": "prettier --check \"{src,__tests__}/**/*.{ts,tsx,scss}\" \"**/*.yml\" \"**/*.json\" \"README.md\" ",
    "lint": "eslint --ext .ts,.tsx,.json .",
    "lint:fix": "eslint --ext .ts,.tsx,.json . --fix",
    "audit": "better-npm-audit audit",
    "test": "npm run audit && npm run format:check && npm run lint && npm run unit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=14.x <=16.x"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@mdx-js/react": "2.1.1",
    "@storybook/addon-actions": "6.5.0-alpha.54",
    "@storybook/addon-essentials": "6.5.0-alpha.54",
    "@storybook/addon-links": "6.5.0-alpha.54",
    "@storybook/builder-vite": "0.1.33",
    "@storybook/jest": "0.0.10",
    "@storybook/node-logger": "6.5.0-alpha.54",
    "@storybook/react": "6.5.0-alpha.54",
    "@storybook/testing-library": "0.0.11",
    "@swc/core": "1.2.183",
    "@swc/jest": "0.2.21",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.2.0",
    "@types/animejs": "3.1.6",
    "@types/jest": "27.5.1",
    "@types/node": "17.0.34",
    "@types/react-dom": "18.0.5",
    "@types/react-test-renderer": "18.0.0",
    "@types/react-transition-group": "4.4.4",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "babel-loader": "8.2.5",
    "better-npm-audit": "3.7.3",
    "env-cmd": "10.1.0",
    "esbuild": "0.14.39",
    "esbuild-jest": "0.5.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "jest": "27.5.1",
    "jest-canvas-mock": "2.4.0",
    "jest-environment-jsdom": "27.5.1",
    "postcss-normalize": "10.0.1",
    "postcss-preset-env": "7.5.0",
    "prettier": "2.6.2",
    "react-test-renderer": "18.1.0",
    "string.prototype.replaceall": "1.0.6"
  }
}
